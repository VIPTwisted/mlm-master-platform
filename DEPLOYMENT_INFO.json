{
  "deployment": {
    "name": "MLM Master Platform",
    "version": "1.0.0",
    "buildDate": "2025-06-02",
    "framework": "Next.js 15.3.2",
    "buildTool": "Bun",
    "outputType": "static"
  },
  "build": {
    "outputDirectory": "out",
    "totalSize": "2.4MB",
    "pageCount": 13,
    "assetOptimization": true,
    "seoOptimized": true,
    "mobileReady": true
  },
  "pages": [
    {
      "path": "/",
      "file": "index.html",
      "title": "MLM Master Platform - Landing Page",
      "description": "Main marketing and signup page"
    },
    {
      "path": "/admin",
      "file": "admin/index.html",
      "title": "Admin Panel",
      "description": "Administrative interface and controls"
    },
    {
      "path": "/dashboard",
      "file": "dashboard/index.html",
      "title": "User Dashboard",
      "description": "Member dashboard with analytics and tools"
    },
    {
      "path": "/advanced-genealogy",
      "file": "advanced-genealogy/index.html",
      "title": "Advanced Genealogy",
      "description": "Team structure visualization and reports"
    },
    {
      "path": "/ai-chatbot",
      "file": "ai-chatbot/index.html",
      "title": "AI Chatbot",
      "description": "AI assistant interface and management"
    },
    {
      "path": "/ai-marketing",
      "file": "ai-marketing/index.html",
      "title": "AI Marketing",
      "description": "AI-powered marketing automation tools"
    },
    {
      "path": "/collaborative-team",
      "file": "collaborative-team/index.html",
      "title": "Collaborative Team",
      "description": "Team collaboration and communication features"
    },
    {
      "path": "/email-campaigns",
      "file": "email-campaigns/index.html",
      "title": "Email Campaigns",
      "description": "Email marketing campaign builder and manager"
    },
    {
      "path": "/marketing-automation",
      "file": "marketing-automation/index.html",
      "title": "Marketing Automation",
      "description": "Comprehensive marketing automation hub"
    },
    {
      "path": "/social-media",
      "file": "social-media/index.html",
      "title": "Social Media Manager",
      "description": "Social media management and scheduling tools"
    },
    {
      "path": "/team-management",
      "file": "team-management/index.html",
      "title": "Team Management",
      "description": "Team administration and performance tracking"
    },
    {
      "path": "/404",
      "file": "404.html",
      "title": "Page Not Found",
      "description": "Custom 404 error page"
    }
