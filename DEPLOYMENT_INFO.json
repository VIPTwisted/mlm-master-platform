  "features": [
    "Responsive design for all devices",
    "SEO optimized with meta tags and structured data",
    "Google Analytics integration ready",
    "Social media sharing optimization",
    "Performance optimized with image preloading",
    "Progressive Web App ready",
    "Accessibility compliant",
    "Modern UI with shadcn/ui components",
    "Advanced genealogy tree visualization",
    "AI-powered marketing tools",
    "Real-time collaboration features",
    "Comprehensive email campaign builder",
    "Social media management interface",
    "Team performance analytics",
    "Admin panel with full controls"
  ],
  "technologies": [
    "Next.js 15.3.2",
    "React 18.3.1",
    "TypeScript 5.8.3",
    "Tailwind CSS 3.4.17",
    "shadcn/ui components",
    "Framer Motion animations",
    "D3.js for data visualization",
    "Recharts for analytics",
    "Lucide React icons"
  ],
  "hosting": {
    "recommended": [
      {
        "service": "Netlify",
        "url": "https://netlify.com",
        "features": ["Free tier", "Automatic SSL", "Global CDN", "Form handling"],
        "buildCommand": "bun run build",
        "publishDirectory": "out"
      },
      {
        "service": "Vercel",
        "url": "https://vercel.com",
        "features": ["Next.js optimized", "Edge network", "Analytics"],
        "buildCommand": "bun run build",
        "publishDirectory": "out"
      },
      {
        "service": "GitHub Pages",
        "url": "https://pages.github.com",
        "features": ["Free hosting", "GitHub integration", "Custom domains"],
        "note": "Upload out/ contents to docs/ folder"
      }
    ]
  },
  "customization": {
    "branding": "Easy to customize colors, logos, and content",
    "content": "All text content can be modified in component files",
    "styling": "Tailwind CSS for rapid customization",
    "analytics": "Replace Google Analytics ID in layout.tsx"
  },
  "requirements": {
    "runtime": "Static hosting (no server required)",
    "ssl": "Recommended for security and SEO",
    "domain": "Custom domain recommended for branding"
  }
